

<h1>Listing Farmacia</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Cedulajur</th>
      <th>Sucursal</th>
      <th>Direccion</th>
      <th>Correo</th>
      <th>Telefono1</th>
      <th>Telefono2</th>
      <th>Latitud</th>
      <th>Longitud</th>
      <th>Cant lab</th>
      <th>Activo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @farmacia.each do |farmacium| %>
      <tr>
        <td><%= farmacium.nombre %></td>
        <td><%= farmacium.cedulaJur %></td>
        <td><%= farmacium.sucursal %></td>
        <td><%= farmacium.direccion %></td>
        <td><%= farmacium.correo %></td>
        <td><%= farmacium.telefono1 %></td>
        <td><%= farmacium.telefono2 %></td>
        <td><%= farmacium.latitud %></td>
        <td><%= farmacium.longitud %></td>
        <td><%= farmacium.cant_lab %></td>
        <td><%= farmacium.activo %></td>
        <td><%= link_to 'Show', farmacium %></td>
        <td><%= link_to 'Edit', edit_farmacium_path(farmacium) %></td>
        <td><%= link_to 'Destroy', farmacium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Farmacium', new_farmacium_path %>
