

<h1 class="text-center">Lista de Farmacias por verificar</h1>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Cedula Jur√≠dica</th>
      <th>Sucursal</th>
      <th>Direccion</th>
      <th>Correo</th>
      <th>Telefono1</th>
      <th>Telefono2</th>
      <th>Cantidad de Laboratorios</th>
    </tr>
  </thead>

  <tbody>
    <% @farmacia.each do |farmacium| %>
      <tr>
        <td><%= farmacium.nombre %></td>
        <td><%= farmacium.cedulaJur %></td>
        <td><%= farmacium.sucursal %></td>
        <td><%= farmacium.direccion %></td>
        <td><%= farmacium.correo %></td>
        <td><%= farmacium.telefono1 %></td>
        <td><%= farmacium.telefono2 %></td>
        <td><%= farmacium.cant_lab%></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<div class="col-lg-8 col-md-8 col-md-offset-4 col-lg-offset-4 col-xs-offset-4 col-sm-offset-4 col-xs-8 col-sm-8 paginate" >
        <%= will_paginate @productos, renderer: BootstrapPagination::Rails %>
    </div>


