<p id="notice"><%= notice %></p>
<%= render 'linkprincipal' %>
<%= bootstrap_form_for(@laboratorio) do |f| %>
<div class="col-lg-6 col-md-8 col-md-offset-2 col-lg-offset-3  col-xs-12 col-sm-12">
	<div class="field">

    <%= f.text_field :nombre ,   :readonly => true%>
  </div>
  <div class="field">
    
    <%= f.text_field :descripcion,  :readonly => true %>
  </div>
</div>

<% end %>
<div class="col-lg-6 col-md-8 col-md-offset-2 col-lg-offset-5 col-xs-12 col-sm-12">

          <a href="/laboratorios/<%= @laboratorio.id %>/edit" class="btn btn-success">
