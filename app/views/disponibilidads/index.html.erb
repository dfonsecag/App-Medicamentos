

<script type="text/javascript">
var msj = "<%= notice %>";
  $(document).ready(function() {
  if(msj!='')
    swal(msj, "", "success")

});
</script>
<h1 class="text-center"> Disponibilidad producto farmacia:</h1>
<div class="col-lg-6 col-md-6 col-md-offset-3 col-lg-offset-3 col-xs-12 col-sm-12">
<table class="table table-striped table-condensed"">
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Acciones</th>
    </tr>
  </thead>

  <tbody>
    <% @disponibilidads.each do |disponibilidad| %>
      <tr id="<%= disponibilidad.id %>">
        <td width="25%"><%= disponibilidad.nombre %></td>
        <td width="10%">
         <a href="/disponibilidads/<%= disponibilidad.id %>/edit" class="btn btn-success">
            <span class="glyphicon glyphicon-edit" aria-hidden="true"></span> Editar
          </a>
           <button class="btn btn-danger" onclick="delete_disponibilidad(<%= disponibilidad.id %>)"><span class="glyphicon glyphicon-trash" aria-hidden="true"></span> Eliminar</button>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>


<div class="col-lg-6 col-md-6 col-md-offset-3 col-lg-offset-3 col-xs-12 col-sm-12">
<%= link_to 'Nueva Disponibilidad', new_disponibilidad_path, :class => "btn btn-success"%>


</div>