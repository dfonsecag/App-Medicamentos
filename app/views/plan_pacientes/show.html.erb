
<%= bootstrap_form_for(@plan_paciente) do |f| %>
 <div class="col-md-6 col-lg-6 col-md-offset-3 col-lg-offset-3  col-xs-12 col-sm-10">
  <div class="field">
    
    <%= f.text_field :nombre,   :readonly => true %>
  </div>
  
     <div class="form-group"><label class="control-label" for="plan_paciente_laboratorio">Laboratorio</label><input readonly="readonly" class="form-control" type="text" value="<%= @plan_paciente.laboratorio.nombre %>" id="plan_paciente_laboratorio" /></div>
  </div>
  
<% end %>
</div>
<div class="col-lg-6 col-md-8 col-md-offset-2 col-lg-offset-5 col-xs-12 col-sm-12">

		 <a href="/plan_pacientes" class="btn btn-info">
         <span class="glyphicon glyphicon-arrow-left" aria-hidden="true"></span>Plan Pacientes
          </a>
          <a href="/plan_pacientes/<%= @plan_paciente.id %>/edit" class="btn btn-success">
            <span class="glyphicon glyphicon-edit" aria-hidden="true"></span> Editar
          </a>
 </div>

