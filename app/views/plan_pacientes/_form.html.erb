<%= bootstrap_form_for(@plan_paciente) do |f| %>
 <div class="col-md-6 col-lg-6 col-md-offset-3 col-lg-offset-3  col-xs-12 col-sm-10">
  <div class="field">
    
    <%= f.text_field :nombre %>
  </div>
  
    <div class="field">
    <br>
    <%= f.collection_select :laboratorio_id, Laboratorio.find_by_sql("Select id, nombre from laboratorios where  not exists (select * from plan_pacientes where plan_pacientes.laboratorio_id = laboratorios.id )"), :id, :nombre , label: "Laboratorios"%>
    
  </div>

  <div class="actions">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>
</div>
